<h1>All The Articles</h1>

<table>
  <thead>
    <tr>
      <td>Title</td>
      <td>URL</td>
      <td>Description</td>
    </tr>
  </thead>

  <tbody>
    <% articles.each do |article| %>
      <tr>
        <td><%= article[:title] %></td>
        <td><%= article[:url] %></td>
        <td><%= article[:description] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
